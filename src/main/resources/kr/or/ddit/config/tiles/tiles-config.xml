<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="layout" template="/WEB-INF/views/common/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/views/common/header.jsp"/>
		<put-attribute name="left" value="/WEB-INF/views/common/left.jsp"/>
		<put-attribute name="body" value=""/>
	</definition>
	
	
	
	<!-- main -->
	<definition name="tiles.main" extends="layout">
		<put-attribute name="body" value="/WEB-INF/views/main.jsp"/>
	</definition>
	
	
	<!-- createBoard View -->
	<definition name="tiles.createBoard" extends="layout">
		<put-attribute name="body" value="/WEB-INF/views/board/createBoard.jsp"/>
	</definition>
	
	<!-- select board View -->
	<definition name="tiles.selectBoard" extends="layout">
		<put-attribute name="body" value="/WEB-INF/views/board/freeBoard.jsp"/>
	</definition>
	
	

	
	<!-- selectText View -->
	<definition name="tiles.textView" extends="layout">
		<put-attribute name="body" value="/WEB-INF/views/board/text.jsp"/>
	</definition>
	
	<!-- deleteText View -->
	<definition name="tiles.delete" extends="layout">
		<put-attribute name="body" value="/WEB-INF/views/board/delete.jsp"/>
	</definition>
	
	
	
	
	
</tiles-definitions>